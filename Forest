package maceraOyunu;

public class Forest extends BattleLoc {

	public Forest(Player player, String name, Obstacle obstacle) {
		super(player, name, obstacle);
	}
	
	public void property(Player player) {
		player.inventory.setFirewood(true);
		System.out.println("Firewood eşyasını kazandınız");
	}
	
		
	}
