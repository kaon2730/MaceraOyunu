package maceraOyunu;

public class GameChar {
	private int damage;
	private int healt;
	private int money;
	
	
	public GameChar(int damage, int healt, int money) {
		this.damage=damage;
		this.healt=healt;
		this.money=money;
	}
	public void print() {
		System.out.println("- " +getClass().getSimpleName());
		System.out.println("Hasar: " + this.damage + "\tSağlık: " + this.healt + "\tPara: " + this.money);
	}
	public int getDamage() {
		return damage;
	}
	public void setDamage(int damage) {
		this.damage = damage;
	}
	public int getHealt() {
		return healt;
	}
	public void setHealt(int healt) {
		this.healt = healt;
	}
	public int getMoney() {
		return money;
	}
	public void setMoney(int money) {
		this.money = money;
	}

}
